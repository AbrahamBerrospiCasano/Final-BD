create table alumno(
ID_Alumno varchar(30) PRIMARY KEY NOT NULL,
Nombre varchar(30) NOT NULL,
Apellido varchar(30) NOT NULL,
Direccion varchar(100) NOT NULL,
Email varchar(50) UNIQUE NOT NULL,
Telefono int UNIQUE NOT NULL,
Facultad varchar(30)
);


